@page
@model ListModel
@{
}

<h1>Restaurants</h1>

<div>
    @Model.Message
</div>

<table class="table">
        <tr>
            <th>Name</th>
            <th>Location</th>
            <th>Cousine type</th>
        </tr>
    @foreach(var restaurant in Model.Restaurants)
    {
        <tr>
            <td>@restaurant.Name</td>
            <td>@restaurant.Location</td>
            <td>@restaurant.Cuisine</td>
        </tr>
    }

</table>


